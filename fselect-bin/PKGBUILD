pkgname=fselect-bin
pkgver=0.7.6
pkgrel=1
arch=('x86_64')
url="https://github.com/jhspetersson/fselect"
license=("MIT")

pkgdesc='Find files with SQL-like queries'

source_x86_64=("fselect-$pkgver.gz::https://github.com/jhspetersson/fselect/releases/download/$pkgver/fselect-$CARCH-linux-musl.gz")
source=("LICENSE::https://raw.githubusercontent.com/jhspetersson/fselect/master/LICENSE-MIT")
sha256sums=('23f18e03dc49df91622fe2a76176497404e46ced8a715d9d2b67a7446571cca3')
sha256sums_x86_64=('c33eeff4791c915b723cdde929c83b5413630f03c2630c3bb78d503cea5f795f')
conflicts=('fselect-git' 'fselect')
provides=('fselect')


package() {
    install -Dm755 "fselect-$pkgver" "$pkgdir/usr/bin/fselect"
    install -Dm644 "LICENSE" "$pkgdir/usr/share/licenses/$pkgname/LICENSE"
}
